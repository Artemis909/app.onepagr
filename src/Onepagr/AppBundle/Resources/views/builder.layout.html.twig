<!DOCTYPE html>
<html>
	<head>

		<title>One page website builder</title>
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<!-- Bootstrap -->
		{% block stylesheets %}
		<link href="{{ asset('assets/bootstrap/css/bootstrap.min.css') }}" rel="stylesheet" media="screen">
		<link href="{{ asset('assets/main/css/animate.min.css') }}" rel="stylesheet" media="screen">
        <link href="{{ asset('assets/main/css/my.css') }}" rel="stylesheet" media="screen">
		
		<link href="{{ asset('assets/builder/css/nav.css') }}" rel="stylesheet" media="screen">
		<link href="{{ asset('assets/builder/css/jquery-ui-1.10.4.min.css') }}" rel="stylesheet" media="screen">
		<script src="{{ asset('assets/jquery/jquery.min.js') }}"></script>
		
		
		
		<script src="{{ asset('assets/main/js/modernizr.custom.js') }}"></script>
		
		
		
		<script src="{{ asset('assets/builder/js/jquery-ui-1.10.4.min.js') }}"></script>
		<script src="{{ asset('assets/builder/js/rangy-core.js') }}"></script>
		<script src="{{ asset('assets/builder/js/hallo.js') }}"></script>
		<script src="{{ asset('assets/builder/js/underscore-min.js') }}"></script>
		<script src="{{ asset('assets/builder/js/backbone-min.js') }}"></script>
		<script src="{{ asset('assets/builder/js/dropzone.js') }}"></script>
		<script src="{{ asset('assets/builder/js/color-thief.min.js') }}"></script>
		<script src="{{ asset('assets/builder/app/onepagr.js') }}"></script>
		
		
		<!--[if lt IE 9]><script src="assets/js/ie8-responsive-file-warning.js"></script><![endif]-->
        <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
        <!--[if lt IE 9]>
          <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
          <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
        <![endif]-->
		{% endblock %}
	</head>

	<body>

		{% block body %}
		{% endblock %}
		<section class="container-fluid">
				{% block page %}

				{% endblock %}
		</section>

		{% block footerscript %}
		<!-- jQuery (necessary for Bootstraps JavaScript plugins) -->
		<!-- Include all JavaScripts, compiled by Assetic -->

		<script src="{{ asset('assets/bootstrap/js/bootstrap.min.js') }}"></script>
		<!-- Animation start -->
		<script src="{{ asset('assets/main/js/wow.min.js') }}"></script>
		<!-- Animation end -->
		<link href="{{ asset('assets/font-awesome/css/font-awesome.min.css') }}" rel="stylesheet" media="screen">
		{% endblock %}
	</body>
</html>